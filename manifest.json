{
  "name": "URL Redirector",
  "description": "Redirect url by pattern.",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://syndication.twitter.com/srv/timeline-profile/screen-name/*",
        "https://platform.twitter.com/embed/Tweet.html?id=*"
      ],
      "css": [
        "css/twitter.css"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://twitter.com/*"
  ]
}
